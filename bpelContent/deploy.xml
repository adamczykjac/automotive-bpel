<?xml version="1.0" encoding="UTF-8"?>
<deploy xmlns="http://www.apache.org/ode/schemas/dd/2007/03" xmlns:orderManagementService="http://www.example.org/orderManagementService/" xmlns:sample="http://eclipse.org/bpel/sample" xmlns:warehouseManagementService="http://www.example.org/warehouseManagementService/">
  <process name="sample:AutoIncOrderProcess">
    <active>true</active>
    <retired>false</retired>
    <process-events generate="all"/>
    <provide partnerLink="client">
      <service name="sample:AutoIncOrderProcessService" port="AutoIncOrderProcessPort"/>
    </provide>
    <invoke partnerLink="OrderMgmtPL">
      <service name="orderManagementService:orderManagementService" port="orderManagementServiceSOAP"/>
    </invoke>
    <invoke partnerLink="WarehouseMgmtPL">
      <service name="warehouseManagementService:warehouseManagementService" port="warehouseManagementServiceSOAP"/>
    </invoke>
  </process>
</deploy>