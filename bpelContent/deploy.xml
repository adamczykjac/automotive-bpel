<?xml version="1.0" encoding="UTF-8"?>
<deploy xmlns="http://www.apache.org/ode/schemas/dd/2007/03" xmlns:WarehouseDecisionWS="http://www.example.org/WarehouseDecisionWS/" xmlns:orderManagementService="http://www.example.org/orderManagementService/" xmlns:sample="http://eclipse.org/bpel/sample" xmlns:warehouseManagementService="http://www.example.org/warehouseManagementService/">
  <process name="sample:OrderingProcess">
    <active>true</active>
    <retired>false</retired>
    <process-events generate="all"/>
    <provide partnerLink="client">
      <service name="sample:OrderingProcessService" port="OrderingProcessPort"/>
    </provide>
    <invoke partnerLink="OrderMgmtCreateOrderPL">
      <service name="orderManagementService:orderManagementService" port="orderManagementServiceSOAP"/>
    </invoke>
    <invoke partnerLink="WarehouseMgmtCheckAvailPL">
      <service name="warehouseManagementService:warehouseManagementService" port="warehouseManagementServiceSOAP"/>
    </invoke>
    <invoke partnerLink="DecisionPL">
      <service name="WarehouseDecisionWS:WarehouseDecisionWS" port="WarehouseDecisionWSSOAP"/>
    </invoke>
  </process>
</deploy>